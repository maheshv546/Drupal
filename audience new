$logo_url = theme_get_setting('logo.url');

// Convert the public URL to a real file path
$logo_path = DRUPAL_ROOT . parse_url($logo_url, PHP_URL_PATH);

// Decode any URL-encoded characters (e.g., %20 â†’ space)
$logo_path = urldecode($logo_path);

if (file_exists($logo_path)) {
  // File exists
  // Do something, maybe log or render
} else {
  \Drupal::logger('debug')->notice('Logo file not found at path: @path', ['@path' => $logo_path]);
}